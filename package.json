{
  "name": "tectonic-explorer",
  "version": "2.0.0",
  "main": "js/components/app.js",
  "scripts": {
    "lint": "eslint \"./js/**/*.{js,jsx,ts,tsx}\" \"./test/**/*.{js,jsx,ts,tsx}\" \"./cypress/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./js/**/*.{js,jsx,ts,tsx}\" \"./test/**/*.{js,jsx,ts,tsx}\" \"./cypress/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch --watchPathIgnorePatterns integration/*",
    "start": "webpack serve",
    "build": "webpack",
    "build:production": "PRODUCTION=true webpack",
    "test:cypress": "cypress run --reporter cypress-image-snapshot/reporter",
    "test:cypress:basic": "cypress run --spec 'cypress/integration/basic/**/*'",
    "test:cypress:snapshots": "cypress run --spec 'cypress/integration/gpu-dependant/**/*' --env failOnSnapshotDiff=false --reporter cypress-image-snapshot/reporter",
    "test:cypress:open": "cypress open --env failOnSnapshotDiff=false",
    "test:cypress:open-with-snapshots": "CYPRESS_COMPARE_SNAPSHOTS=true cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/tectonic-explorer.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/tectonic-explorer/issues"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "testURL": "https://tectonic-explorer.jest-test.com",
    "setupFilesAfterEnv": [
      "<rootDir>test/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.svg$": "<rootDir>/__mocks__/svgMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "homepage": "https://github.com/concord-consortium/tectonic-explorer#readme",
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.1.0",
    "array-move": "^3.0.1",
    "d3-color": "^2.0.0",
    "d3-hsv": "^0.1.0",
    "d3-interpolate": "^2.0.1",
    "d3-scale": "^3.3.0",
    "eventemitter2": "^6.4.5",
    "firebase": "^8.10.0",
    "jquery": "^3.6.0",
    "kd-tree-javascript": "^1.0.3",
    "lodash": "^4.17.21",
    "mobx": "^6.3.7",
    "mobx-react": "^7.2.1",
    "point-in-polygon": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-draggable": "^4.4.4",
    "react-toolbox": "2.0.0-beta.13",
    "react-transition-group": "~4.4.2",
    "rollbar": "^2.24.0",
    "screenfull": "^5.1.0",
    "seedrandom": "^3.0.5",
    "shutterbug": "^1.3.2",
    "three": "^0.130.1",
    "timeseries-analysis": "^1.0.12",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/d3-color": "^2.0.2",
    "@types/d3-hsv": "^0.1.4",
    "@types/d3-interpolate": "^2.0.2",
    "@types/d3-scale": "^3.3.2",
    "@types/enzyme": "^3.10.10",
    "@types/jest": "^27.0.2",
    "@types/jquery": "^3.5.8",
    "@types/kd-tree-javascript": "^1.0.0",
    "@types/lodash": "^4.14.177",
    "@types/point-in-polygon": "^1.1.1",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-transition-group": "^4.4.4",
    "@types/seedrandom": "^3.0.1",
    "@types/three": "^0.130.2",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "canvas": "^2.8.0",
    "copy-webpack-plugin": "^9.1.0",
    "css-loader": "^6.5.1",
    "cypress": "^9.0.0",
    "cypress-image-snapshot": "^4.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.2.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "get-pixels": "^3.3.3",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^3.1.1",
    "jest": "^27.3.1",
    "json-loader": "^0.5.7",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "postcss": "^8.3.11",
    "postcss-cssnext": "^3.1.1",
    "postcss-loader": "^6.2.0",
    "raw-loader": "^4.0.2",
    "react-sortable-hoc": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "url-loader": "^4.1.1",
    "wait-on": "^6.0.0",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  }
}
