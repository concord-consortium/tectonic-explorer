{
  "name": "tectonic-explorer",
  "version": "1.4.0",
  "main": "js/components/app.js",
  "scripts": {
    "lint": "eslint \"./js/**/*.{js,jsx,ts,tsx}\" \"./test/**/*.{js,jsx,ts,tsx}\" \"./cypress/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./js/**/*.{js,jsx,ts,tsx}\" \"./test/**/*.{js,jsx,ts,tsx}\" \"./cypress/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch --watchPathIgnorePatterns model-serialization.test.js",
    "start": "webpack serve",
    "build": "webpack",
    "build-production": "PRODUCTION=true webpack",
    "test:cypress": "cypress run --reporter cypress-image-snapshot/reporter",
    "test:cypress:basic": "cypress run --spec cypress/integration/basic/**/*",
    "test:cypress:snapshots": "cypress run --spec cypress/integration/gpu-dependant/**/* --env failOnSnapshotDiff=false --reporter cypress-image-snapshot/reporter",
    "test:cypress:open": "cypress open --env failOnSnapshotDiff=false",
    "test:cypress:open-with-snapshots": "CYPRESS_COMPARE_SNAPSHOTS=true cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/tectonic-explorer.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/tectonic-explorer/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "testURL": "https://tectonic-explorer.jest-test.com",
    "setupFilesAfterEnv": [
      "<rootDir>test/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "homepage": "https://github.com/concord-consortium/tectonic-explorer#readme",
  "dependencies": {
    "array-move": "^3.0.1",
    "d3-color": "^2.0.0",
    "d3-hsv": "^0.1.0",
    "d3-interpolate": "^2.0.1",
    "d3-scale": "^3.2.3",
    "eventemitter2": "^6.4.3",
    "firebase": "^8.2.8",
    "jquery": "^3.5.1",
    "kd-tree-javascript": "^1.0.3",
    "lodash": "^4.17.20",
    "mobx": "^6.1.7",
    "mobx-react": "^7.1.0",
    "point-in-polygon": "^1.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-toolbox": "2.0.0-beta.13",
    "react-transition-group": "~4.4.1",
    "rollbar": "^2.21.0",
    "screenfull": "^5.1.0",
    "seedrandom": "^3.0.5",
    "shutterbug": "^1.3.2",
    "three": "^0.119.1",
    "timeseries-analysis": "^1.0.12",
    "ts-migrate": "^0.1.15",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/d3-color": "^2.0.1",
    "@types/d3-hsv": "^0.1.4",
    "@types/d3-interpolate": "^2.0.0",
    "@types/d3-scale": "^3.2.2",
    "@types/enzyme": "^3.10.8",
    "@types/jquery": "^3.5.5",
    "@types/kd-tree-javascript": "^1.0.0",
    "@types/lodash": "^4.14.168",
    "@types/point-in-polygon": "^1.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-transition-group": "^4.4.1",
    "@types/seedrandom": "^2.4.28",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "cypress": "^6.5.0",
    "cypress-image-snapshot": "^4.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.20.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "get-pixels": "^3.3.2",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^2.0.0",
    "jest": "^26.6.3",
    "json-loader": "^0.5.7",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "postcss": "^8.2.6",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^5.0.0",
    "raw-loader": "^4.0.2",
    "react-sortable-hoc": "^1.11.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "url-loader": "^4.1.1",
    "wait-on": "^5.2.1",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
