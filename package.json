{
  "name": "tectonic-explorer",
  "version": "2.3.0",
  "main": "js/components/app.js",
  "scripts": {
    "lint": "eslint \"./js/**/*.{js,jsx,ts,tsx}\" \"./test/**/*.{js,jsx,ts,tsx}\" \"./cypress/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"./js/**/*.{js,jsx,ts,tsx}\" \"./test/**/*.{js,jsx,ts,tsx}\" \"./cypress/**/*.{js,jsx,ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch --watchPathIgnorePatterns integration/*",
    "start": "webpack serve",
    "build": "webpack",
    "build:production": "PRODUCTION=true webpack",
    "test:cypress": "cypress run --reporter cypress-image-snapshot/reporter",
    "test:cypress:basic": "cypress run --spec 'cypress/integration/basic/**/*'",
    "test:cypress:snapshots": "cypress run --spec 'cypress/integration/gpu-dependant/**/*' --env failOnSnapshotDiff=false --reporter cypress-image-snapshot/reporter",
    "test:cypress:open": "cypress open --env failOnSnapshotDiff=false",
    "test:cypress:open-with-snapshots": "CYPRESS_COMPARE_SNAPSHOTS=true cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/concord-consortium/tectonic-explorer.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/concord-consortium/tectonic-explorer/issues"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "https://tectonic-explorer.jest-test.com"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(array-move|d3-color|d3-interpolate|d3-scale|screenfull)/)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>test/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.svg$": "<rootDir>/__mocks__/svgMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "homepage": "https://github.com/concord-consortium/tectonic-explorer#readme",
  "dependencies": {
    "@concord-consortium/lara-interactive-api": "^1.5.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.0",
    "array-move": "^4.0.0",
    "d3-color": "^3.1.0",
    "d3-hsv": "^0.1.0",
    "d3-interpolate": "^3.0.1",
    "d3-scale": "^4.0.2",
    "eventemitter2": "^6.4.5",
    "firebase": "^9.8.1",
    "jquery": "^3.6.0",
    "kd-tree-javascript": "^1.0.3",
    "lodash": "^4.17.21",
    "mobx": "^6.6.0",
    "mobx-react": "^7.5.0",
    "point-in-polygon": "^1.1.0",
    "query-string": "^7.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-draggable": "^4.4.5",
    "react-tabs": "^5.1.0",
    "react-toolbox": "2.0.0-beta.13",
    "react-transition-group": "~4.4.2",
    "rollbar": "^2.25.0",
    "screenfull": "^6.0.1",
    "seedrandom": "^3.0.5",
    "shutterbug": "^1.3.2",
    "three": "^0.131.3",
    "timeseries-analysis": "^1.0.12",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/d3-color": "^3.0.2",
    "@types/d3-hsv": "^0.1.4",
    "@types/d3-interpolate": "^3.0.1",
    "@types/d3-scale": "^4.0.2",
    "@types/jest": "^27.5.1",
    "@types/jquery": "^3.5.14",
    "@types/kd-tree-javascript": "^1.0.1",
    "@types/lodash": "^4.14.182",
    "@types/point-in-polygon": "^1.1.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-tabs": "^5.0.5",
    "@types/react-transition-group": "^4.4.4",
    "@types/seedrandom": "^3.0.2",
    "@types/three": "^0.131.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "canvas": "^2.9.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "cypress": "^9.6.1",
    "cypress-image-snapshot": "^4.0.1",
    "eslint": "^8.16.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "file-loader": "^6.2.0",
    "get-pixels": "^3.3.3",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^3.1.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "json-loader": "^0.5.7",
    "less": "^4.1.2",
    "less-loader": "^11.0.0",
    "postcss": "^8.4.14",
    "postcss-cssnext": "^3.1.1",
    "postcss-loader": "^7.0.0",
    "raw-loader": "^4.0.2",
    "react-sortable-hoc": "^2.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "url-loader": "^4.1.1",
    "wait-on": "^6.0.1",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  }
}
